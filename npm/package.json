{
  "name": "@penumbra-labs/registry",
  "version": "11.3.1",
  "description": "Chain and asset registry for Penumbra",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "format": "prettier . --write",
    "format-check": "prettier --check .",
    "lint": "eslint . --max-warnings=0",
