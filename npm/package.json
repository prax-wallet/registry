    "changeset": "changeset",
    "changeset:publish": "changeset publish",
    "test": "vitest run"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^1.10.0",
    "@changesets/cli": "^2.27.8",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@penumbra-zone/protobuf": "^6.0.0",
    "eslint": "^9.10.0",
    "fetch-mock": "^11.1.4",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.6",
    "vite-plugin-dts": "^4.2.1",
    "vitest": "^2.1.1"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ]
